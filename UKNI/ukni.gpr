project UKNI is

   for Source_Dirs use (".", "AVR");
   for Object_Dir use "../IB_Phase3_Build/";
   for Main use ("ib_main.adb", "main.adb");

   package Naming is
      for Body_Suffix ("ada") use ".adb";
      for Separate_Suffix use ".adb";
      for Spec ("timer") use "timer.ads";
      for Body ("timer") use "timer.shw";
      for Spec ("memory_checks") use "memory_checks.ads";
      for Body ("memory_checks") use "memory_checks.shw";
   end Naming;

end UKNI;

